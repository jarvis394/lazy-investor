{
  "name": "nestjs-prisma-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "jarvis394",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev:front": "yarn nx run frontend:serve",
    "dev:back": "yarn nx run backend:serve",
    "dev": "yarn nx run-many -t serve -p frontend backend",
    "build:front": "yarn nx run frontend:build",
    "build:back": "yarn nx run backend:build",
    "build": "yarn nx run-many -t build",
    "lint": "yarn nx run-many -t lint",
    "prepare": "husky",
    "prepare:db": "yarn prisma migrate dev"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/platform-fastify": "^10.3.9",
    "@prisma/client": "^5.15.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.6.1",
    "@swc/helpers": "^0.5.11",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nestjs-prisma": "^0.23.0",
    "nestjs-telegraf": "^2.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^5.15.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "telegraf": "^4.16.3",
    "telegram": "^2.22.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@nx/esbuild": "^18.2.2",
    "@nx/eslint-plugin": "^18.2.2",
    "@nx/js": "^18.2.2",
    "@nx/linter": "^18.2.2",
    "@nx/nest": "^18.2.2",
    "@nx/node": "^18.2.2",
    "@nx/react": "^18.2.2",
    "@nx/webpack": "^18.2.2",
    "@nx/workspace": "^18.2.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^9.0.11",
    "jest": "29.3.1",
    "nx": "^18.2.2",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
